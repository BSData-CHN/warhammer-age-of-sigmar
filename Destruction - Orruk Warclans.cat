    <profile id="1e2c-6b55-1e70-ce34" name="Foot of Gork" page="114" hidden="false" typeId="2e81-5e22-c6e1-73cb" typeName="Spell">
      <characteristics>
        <characteristic name="Casting Value" typeId="2508-b604-1258-a920">10</characteristic>
